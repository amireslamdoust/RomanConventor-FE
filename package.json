{
  "name": "exhanger",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@loadable/component": "^5.13.2",
    "env-cmd": "^10.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "react": "^16.9.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-transition-group": "^4.4.1"
  },
  "scripts": {
    "build:style": "postcss src/styles/index.css -o src/styles/main.css",
    "start": "npm run build:style && react-scripts start",
    "build": "npm run build:style && react-scripts build",
    "test": "env-cmd -f ./.env.test react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --config .prettierrc --write src/**/*.js",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "e2e": "cypress open",
    "cy:run": "cypress run",
    "teste2e": "start-server-and-test start http://localhost:3000 cy:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "@types/jest": "^26.0.14",
    "@types/loadable__component": "^5.13.0",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.6",
    "cypress": "^5.2.0",
    "husky": "^4.3.0",
    "locales": "^0.0.2",
    "postcss-cli": "^7.1.2",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.3",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.7.4"
  }
}
